{
  "name": "agent-starter-node",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,js}\"",
    "lint:fix": "eslint --fix \"**/*.{ts,js}\"",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "test": "node --test --import tsx",
    "test:watch": "node --test --watch --import tsx",
    "dev": "tsx src/agent.ts dev",
    "download-files": "pnpm run build && node dist/agent.js download-files",
    "start": "node dist/agent.js start",
    "server": "node server.js",
    "dev-server": "tsx server.js",
    "visual": "pnpm run dev-server",
    "start-visual": "node start-visual-system.js",
    "full-system": "node start-visual-system.js",
    "socketio-server": "node server-socketio.js",
    "token-server": "node server-with-tokens.js",
    "enhanced-agent": "tsx src/agent-enhanced.ts dev",
    "deploy": "tsx src/agent-enhanced.ts start",
    "complete-system": "node start-complete-system.js"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.18.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "dependencies": {
    "@livekit/agents": "^1.0.7",
    "@livekit/agents-plugin-livekit": "^1.0.7",
    "@livekit/agents-plugin-silero": "^1.0.7",
    "@livekit/noise-cancellation-node": "^0.1.9",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "livekit-client": "2.15.8",
    "livekit-server-sdk": "^2.14.0",
    "node-fetch": "^3.3.2",
    "sharp": "^0.34.4",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "zod": "^3.25.76"
  }
}
